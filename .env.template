# =====================
# Slack Notifications
# =====================
SLACK_HOOK=<your-webhook-url>

# =====================
# OpenAI API
# =====================
OPENAI_API_KEY=<your-openai-api-key>

# =====================
# Pinecone Vector DB
# =====================
PINECONE_API_KEY=<your-pinecone-api-key>
PINECONE_INDEX_NAME=<your-pinecone-index-name>
PINECONE_NAMESPACE=<your-pinecone-namespace>

# =====================
# Pipeline Controls
# =====================
DRY_RUN_LIMIT=2
THROTTLE_SEC=1.0
BATCH_SIZE=200

# =====================
# VAPI (Voice API, if used)
# =====================
VAPI_TOKEN=<your-vapi-token>

# =====================
# AWS Configuration
# =====================
AWS_ACCESS_KEY_ID=<your-aws-access-key-id>
AWS_SECRET_ACCESS_KEY=<your-aws-secret-access-key>
AWS_REGION=<your-aws-region>
S3_BUCKET_NAME=<your-s3-bucket-name>

# =====================
# Neo4j Configuration
# =====================
NEO4J_URI=<your-neo4j-uri>
NEO4J_USER=<your-neo4j-username>
NEO4J_PASSWORD=<your-neo4j-password>

# =====================
# Postgres Configuration
# =====================
POSTGRES_DSN=<your-postgres-dsn>

# =====================
# Additional (add as needed)
# =====================
# OTHER_SERVICE_KEY=<your-other-service-key>